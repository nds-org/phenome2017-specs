{
    "key": "rstudio",
    "label": "RStudio Server",
    "description": "IDE for the R programming language.",
    "logo": "/asset/png/logos/rstudio-logo.png",
    "image": {
        "name": "rocker/ropensci",
        "tags": [
            "latest",
            "dev"
        ]
    },
    "display": "stack",
    "access": "external",
    "ports": [
        {
            "port": 8787,
            "protocol": "http"
        }
    ],
    "repositories": [
        {
            "url": "https://github.com/rstudio/rstudio",
            "type": "git"
        }
    ],
    "readinessProbe": {
        "type": "http",
        "path": "/images/rstudio.png",
        "port": 8787,
        "initialDelay": 1,
        "timeout": 360
    },
    "volumeMounts": [
        {
            "type": "",
            "mountPath": "/home/rstudio"
        }
    ],
    "resourceLimits": {
        "cpuMax": 500,
        "cpuDefault": 100,
        "memMax": 2000,
        "memDefault": 50
    },
    "tags": [ "7", "28", "40" ],
	"authRequired": true,
    "info": "https://nationaldataservice.atlassian.net/wiki/display/NDSC/RStudio"
}
